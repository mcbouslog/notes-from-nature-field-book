{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier",
    "react"
  ],
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "eqeqeq": [
      2,
      "smart"
    ],
    "react/prefer-stateless-function": [1],
    "react/jsx-filename-extension": [1, {
      "extensions": [".js", ".jsx"]
    }],
    "keyword-spacing": 2,
    "comma-dangle": ["warn", "never"],
    "no-console": 1,
    "max-len": [1, 120, 2, {
      "ignoreUrls": true,
      "ignoreComments": false
    }],
    "space-before-function-paren": ["error", "never"]
  }
}